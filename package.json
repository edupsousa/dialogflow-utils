{
  "name": "dialogflow-utils",
  "version": "0.1.0",
  "description": "Utilities to work with Google DialogFlow Backup files.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "df-utils": "lib/index.js"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "local-install": "npm run build && npm install -g",
    "test": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' --quiet --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && npm run docs && git add -A src docs",
    "postversion": "git push && git push --tags",
    "docs": "typedoc && git add -A docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edupsousa/dialogflow-utils.git"
  },
  "keywords": [
    "dialogflow",
    "backup",
    "import",
    "agent",
    "intents",
    "entities",
    "training",
    "phrases"
  ],
  "author": "Eduardo P de Sousa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edupsousa/dialogflow-utils/issues"
  },
  "homepage": "https://github.com/edupsousa/dialogflow-utils#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "files": [
    "lib/**/*"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/figlet": "^1.2.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.29",
    "typedoc-plugin-markdown": "^3.6.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.1.0",
    "dialogflow-import": "^0.1.2",
    "figlet": "^1.5.0"
  }
}
